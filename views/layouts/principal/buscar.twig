
		<table id="myTable" class="table">
		 	<thead>
		 		<th>#PROCESO</th>
		 		<th>CIUDAD</th>
		 		<th>NOMBRE EMPRESA</th>
		 		<th>DEMANDANTE</th>
		 		<th>DEMANDADO</th>
		 		<th>VALOR</th>
		 		<th>ADJUNTAR</th>
		 	</thead>
		 	<tbody>
		 		{% for result in results  %}		 	
		 			<tr>
		 				<td>{{result.num_proceso}}</td>
		 				<td>{{result.ciudad}}</td>
		 				<td>{{result.nom_empresa}}</td>
		 				<td>{{result.demandante~' ('~result.cc_demandante~')'}}</td>
		 				<td>{{result.demandado~' ('~result.cc_demandado~')'}}</td>
		 				<td>${{result.valor}}.oo</td>
		 				<td><a  class="btn btn-success" href="views/usuarios.php?id={{result.id_proceso}}">Ver</a></td>
		 			</tr>										
		 		{% endfor %}
		 	</tbody>
		 </table>